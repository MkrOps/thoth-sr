  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-chrome",
      "request": "launch",
      "name": "Launch Chrome against localhost",
      "url": "http://localhost:3001",
      "sourceMap": true,
      "webRoot": "${workspaceFolder}/client",
      "trace": true,
      "sourceMapPathOverrides": {
        "webpack://thothCore/./*": "${workspaceFolder}/core/*",
        "webpack://@thoth/client/./*": "${workspaceFolder}/client/*"
      }
    }
  ]
}

