{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "module": "es2015",
    "jsx": "react-jsx",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "strict": false,
    "outDir": "./dist/",
    "declaration": true,
    "composite": true,
    "noEmit": false,
    "target": "es6",
    "sourceMap": true
  },
  "include": ["./"],
  "exclude": ["./node_modules/", "./dist/", "./rollup.config.js"]
}
